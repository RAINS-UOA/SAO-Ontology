}, {
  "@id" : "https://w3id.org/sao#AccountabilityPlan",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A plan which describes a specification of the information that should be collected, so as to support accountability of systems."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Accountability Plan"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/ep-plan#Plan"
  } ]
}, {
  "@id" : "https://w3id.org/sao#AccountableAction",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A planned step which is deemed to be important for accountability purposes and produces AccountableResult."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Accountable Action"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/ep-plan#MultiStep"
  } ]
}, {
  "@id" : "https://w3id.org/sao#AccountableAgent",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "An agent (typically a human)  that can be held accountable for his/her actions."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Accountable Agent"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/ep-plan#Agent"
  }, {
    "@id" : "https://w3id.org/sao#InformationElement"
  } ]
}, {
  "@id" : "https://w3id.org/sao#AccountableObject",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "An accountable object models an abstract representation of a thing that should be documented for accountability. This may reference the whole system, its logical part, or associated process.  For example, a larger system may be described as a group of sub-systems or a single system may be broken down into a number of layers/components (e.g., a decision logic layer)."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Accountable Object"
  } ]
}, {
  "@id" : "https://w3id.org/sao#AccountableResult",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "An output variable which is produced by an Accountable Action, for example,  a decision made by a human or a specification produced by a team. The variable is part of the accountability plan and has a corresponding InformationRealization collection in the execution trace."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Accountable Result"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/ep-plan#MultiVariable"
  }, {
    "@id" : "_:genid1"
  } ]
}, {
  "@id" : "https://w3id.org/sao#InformationElement",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A member of the collection of type Information Realization. Information Realizations may have multiple Information Elements.  Information Element describes in more detail a specific type of information contained by the Information Realization collection."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Information Element"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/ep-plan#Entity"
  } ]
}, {
  "@id" : "https://w3id.org/sao#InformationRealization",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@value" : "A collection of pieces of information (InformationElements) which corresponds to a variable of type  AccountableResult. The collection represents the actual piece of provenance information documenting a specific AccountableResult during a single plan execution."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Information Realization"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/ep-plan#EntityCollection"
  }, {
    "@id" : "_:genid2"
